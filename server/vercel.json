{
  "version": 2,
  "builds": [
    { "src": "api/properties.js", "use": "@vercel/node" },
    { "src": "api/user.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/properties", "dest": "api/properties.js" },
    { "src": "/api/user", "dest": "api/user.js" },
    { "src": "/(.*)", "dest": "public/$1" }
  ]
}
